# Listmonk Environment Configuration
# Copy this file to .env and fill in your values
# IMPORTANT: Never commit the .env file to the repository!

# PostgreSQL Database Credentials
DB_USER=listmonk
DB_PASSWORD=your_secure_database_password_here
DB_NAME=listmonk

# Listmonk Admin Credentials (only used on first setup)
# These create the initial super admin user
LISTMONK_ADMIN_USER=admin
LISTMONK_ADMIN_PASSWORD=your_secure_admin_password_here

# Timezone (optional, defaults to Europe/Berlin)
TZ=Europe/Berlin
